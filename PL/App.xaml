<Application x:Class="PL.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PL"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:CallsCollection x:Key="CallsCollectionKey" />
        <local:RolesCollection x:Key="RolesCollectionKey" />
        <local:DistanceTypeCollection x:Key="DistanceTypeCollectionKey" />
        <local:IdToReadOnlyConverter x:Key="IdToReadOnlyConverter" />
        <local:DistanceTypeToLabelConverter x:Key="DistanceTypeToLabelConverter" />
        <local:NullToFalseConverter x:Key="NullToFalseConverter" />
    </Application.Resources>
</Application>
